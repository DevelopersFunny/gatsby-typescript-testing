{
  "name": "gatsby-typescript-testing",
  "version": "1.0.0",
  "private": true,
  "description": "A project with React GatsbyJS and TypeScript with Testing using Atomic Design",
  "author": "Developers Funny",
  "keywords": [
    "gatsby",
    "react",
    "jest",
    "testing",
    "test",
    "storybook",
    "github pages",
    "typescript",
    "tailwind",
    "tailwindcss",
    "tailwind-css",
    "styled-components",
    "twin.macro"
  ],
  "scripts": {
    "develop:gatsby": "gatsby develop",
    "develop:type-check": "tsc --watch",
    "develop": "npm-run-all --parallel develop:gatsby develop:type-check",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "jest",
    "test:coverage": "jest --watchAll --coverage",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json"
  },
  "babelMacros": {
    "twin": {
      "preset": "styled-components"
    }
  },
  "dependencies": {
    "@headlessui/react": "^1.4.3",
    "@heroicons/react": "^1.0.5",
    "babel-plugin-styled-components": "^2.0.2",
    "gatsby": "^4.5.3",
    "gatsby-plugin-image": "^2.5.2",
    "gatsby-plugin-manifest": "^4.5.2",
    "gatsby-plugin-react-helmet": "^5.5.0",
    "gatsby-plugin-sharp": "^4.5.2",
    "gatsby-plugin-styled-components": "^5.5.0",
    "gatsby-plugin-typescript": "^4.5.2",
    "gatsby-source-filesystem": "^4.5.2",
    "gatsby-transformer-remark": "^5.5.2",
    "gatsby-transformer-sharp": "^4.5.0",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.3.3",
    "tailwind-tales-addon": "^1.0.0-alpha.0",
    "twin.macro": "^2.8.2",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@storybook/addon-a11y": "^6.4.13",
    "@storybook/addon-actions": "^6.4.13",
    "@storybook/addon-essentials": "^6.4.13",
    "@storybook/addon-jest": "^6.4.13",
    "@storybook/addon-links": "^6.4.13",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.4.13",
    "@storybook/manager-webpack5": "^6.4.13",
    "@storybook/react": "^6.4.13",
    "@storybook/testing-react": "^1.2.3",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.9",
    "@types/puppeteer": "^5.4.4",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-test-renderer": "^17.0.1",
    "autoprefixer": "^10.4.2",
    "babel-jest": "^27.4.6",
    "babel-loader": "^8.2.3",
    "babel-preset-gatsby": "^2.5.2",
    "gatsby-plugin-postcss": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.7",
    "jest-puppeteer": "^6.0.3",
    "jest-screenshot": "^0.3.5",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "prettier": "2.5.1",
    "puppeteer": "^13.1.1",
